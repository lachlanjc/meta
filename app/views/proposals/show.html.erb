<% title 'Proposals' %>
<% activate_nav! :people %>
<% activate_nav! :people_governance %>

<%= render 'people/nav' %>

<div class="sheet-body">
  <div class="row">
    <div class="md-col-12 sm-col-12">
      <%= react_component(
      'Proposal',
      newsFeedItem: @proposal.news_feed_item,
      user: UserSerializer.new(@proposal.user),
      proposal: ProposalSerializer.new(@proposal),
      user_vote_state: @proposal.user_vote_status(current_user)
      ) %>
    </div>
  </div>
</div>
